Xcursor.theme: ComixCursors-Black-Regular

urxvt*font:             xft:Liberation Mono:antialias=true:pixelsize=10
!urxvt*font:             xft:FreeMono:style=medium:antialias=true:pixelsize=10
!urxvt*font:             xft:Inconsolata:antialias=true:pixelsize=10
!urxvt*font:             xft:Bitstream Vera Sans Mono:pixelsize=12

urxvt*termName:         rxvt
urxvt*background:       #DDDDDD
urxvt*foreground:       #222222
URxvt.tabbed.tabbar-fg: 8
URxvt.tabbed.tabbar-bg: -1
URxvt.tabbed.tab-fg: 1
URxvt.tabbed.tab-bg: -1

urxvt*scrollBar:        false
urxvt*perl-lib:         /usr/lib/urxvt/perl/
!urxvt*perl-ext-common:  default,matcher,tabbed,searchable-scrollback
urxvt*perl-ext-common:  default,matcher,searchable-scrollback
urxvt*urlLauncher:      /bin/bin/firefox
urxvt*matcher.button:   1

!urxvt*fading:           50
!urxvt*fadeColor:        #444444
URxvt.transparent:   true
!URxvt.tintColor:     #dcdcdc
URxvt.shading:       165
URxvt.geometry:		80x20


Xft*dpi:                120
Xft*antialias:          true
Xft*hinting:            full


xterm*title: xterm
xterm*faceName: terminus:pixelsize=12
!xterm*faceName: cure:Regular
!xterm*boldfaceName: cure:Regular
!xterm*faceName: Pragmata
!xterm*faceSize: 8.7
!xterm*boldfaceName: Pragmata
xterm*geometry: 100x30
!xterm*boldMode: false
xterm*cutNewline: false
xterm*on4Clicks: group
xterm*on5Clicks: page
xterm*internalBorder: 1
!xterm*background: #262729
xterm*background: white
xterm*foreground: black
xterm*metaSendsEscape: true
xterm*colorBD: #ffffff
xterm*colorBDMode: false
xterm*cursorBlink: false
xterm*cursorColor: #e2baf1
!xterm*veryBoldColors: 14
xterm*dynamiccolors: true
xterm*utf8: 1
xterm*loginShell: true
xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48,35:48,58:48,63:48,61:48,44:48,38:48,59:48
xterm*jumpScroll: true
xterm*multiScroll: true
xterm*saveLines: 512


